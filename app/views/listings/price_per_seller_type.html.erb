<div class=container>

  <div class="container mt-5">
    <h2><i class="fas fa-search-dollar"></i> Average Listing Selling Price per Seller Type</h2>
    <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">Seller Type</th>
          <th scope="col">Average in Euro</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>private</td>
          <td>€<%= number_with_delimiter(@privates_price, :delimiter => ',') %>,-</td>
        </tr>
        <tr>
          <td>dealer</td>
          <td>€<%= number_with_delimiter(@dealers_price, :delimiter => ',') %>,-</td>
        </tr>
        <tr>
          <td>other</td>
          <td>€<%= number_with_delimiter(@others_price, :delimiter => ',') %>,-</td>
        </tr>
      </tbody>
    </table>

    <div><%= link_to "Back", root_path %></div>

  </div>
</div>
